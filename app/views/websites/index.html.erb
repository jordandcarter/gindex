<h1>Listing websites</h1>

<table>
  <tr>
    <th>Url</th>
    <th>Last Count</th>
    <th>Last Count At</th>
  </tr>

<% @websites.each do |website| %>
  <tr>
    <td><%=h website.url %></td>
    <td><%=h website.counts.last.nil? ? "na" : website.counts.last.count %></td>
    <td><%=h website.counts.last.nil? ? "na" : website.counts.last.created_at %></td>
    <td><%= link_to 'Show', website %></td>
    <td><%= link_to 'Edit', edit_website_path(website) %></td>
    <td><%= link_to 'Destroy', website, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New website', new_website_path %>
