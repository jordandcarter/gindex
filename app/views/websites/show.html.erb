<% content_for :head do %>
<% end %>

<div id="placeholder" style="height: 400px; width:600px"></div>
<br/>
<%= link_to 'Edit', edit_website_path(@website) %> |
<%= link_to 'Back', websites_path %>
<script type="text/javascript">
$(document).ready(function(){
      $.plot($("#placeholder"), [
         { label: "<%= @website.url %>", data: [ <%=@website.counts.from_date(1.month.ago).collect{|count| "[#{count.created_at.to_i*1000}, #{count.count}]"}.join(",") %>]}
      ], {xaxis:{mode: "time"}} );
});
</script>
